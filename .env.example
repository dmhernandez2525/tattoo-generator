# INK SYNTHESIS - Environment Variables
# =====================================
# Copy this file to .env and update values as needed.

# Node environment
NODE_ENV=development

# AI Generation API (for future integration)
# VITE_AI_API_URL=http://localhost:3001
# VITE_AI_API_KEY=your-api-key-here
# VITE_AI_MODEL=stable-diffusion-xl

# Machine Hardware Connection (for future hardware integration)
# VITE_MACHINE_API_URL=http://localhost:8080
# VITE_MACHINE_SERIAL_PORT=/dev/ttyUSB0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_MOCK_GENERATION=true
